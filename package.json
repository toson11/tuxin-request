{
  "name": "@tuxinlab/request",
  "version": "1.0.4",
  "description": "基于 axios 的请求库，支持 TypeScript，支持请求缓存、请求重试、请求加载动画、请求参数加密、响应数据脱敏、重复请求拦截等",
  "homepage": "https://github.com/toson11/tuxin-request",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "release": "npm version patch && npm publish --access public && git push && git push --tags"
  },
  "keywords": [
    "axios",
    "request",
    "http",
    "typescript"
  ],
  "author": "longtuxin",
  "license": "MIT",
  "dependencies": {
    "@types/crypto-js": "^4.2.2"
  },
  "peerDependencies": {
    "axios": "^1.4.0",
    "crypto-js": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "axios": "^1.4.0",
    "crypto-js": "^4.0.0",
    "eslint": "^8.56.0",
    "rollup": "^4.12.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
